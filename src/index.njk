---
title: Maeibh Sheehan Artist
layout: "layout.njk"
tag: "home"

---

<main>
    <div class="glide mb-4">
        <div data-glide-el="track" class="glide__track">
            <ul class="frames__list glide__slides">
                {%- for slides in collections.slides -%}
                <li class="glide__slide">
                    <img src="/assets/images/slides/{{ slides.data.image }}" class="img-fluid w-100 cu-front-image"
                        height="600" width="551" alt="">
                </li>
                {%- endfor -%}
            </ul>
        </div>
    </div>
    <div class="container">

        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4 g-4">
            {%- for pieces in collections.pieces -%}
            <div class="col">
                <div class="card shadow-sm">
                    <div class="cu-image-container" tabindex="0">
                        <img class="cu-front-thumbs" src="/assets/images/full/{{ pieces.data['image'] }}"
                            alt="{{ pieces.data.imageAlt }}" width="330" height="400">
                        <div class="cu-image-title p-1">
                            <h3>{{ pieces.data.title }}</h3>
                        </div>
                    </div>
                    <div class="card-body">
                        <p class="card-text">{{ pieces.data.description }}</p>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="btn-group">
                                <button type="button" class="btn btn-sm btn-outline-primary">View</button>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            {%- endfor -%}
        </div>
    </div>
</main>