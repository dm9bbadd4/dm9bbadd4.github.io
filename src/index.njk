---
title: Maeibh Sheehan Artist
layout: "layout.njk"
tag: "home"

---

<main>
    <div class="glide mb-2">
        <div data-glide-el="track" class="glide__track">
            <ul class="frames__list glide__slides">
                {%- for slides in collections.slides -%}
                <li class="glide__slide">
                    <img src="/assets/images/slides/{{ slides.data.image }}" class="img-fluid w-100 cu-front-image" height="500" alt="">
                </li>
                {%- endfor -%}
            </ul>
        </div>
    </div>
    <div class="container">

        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-4">
            {%- for pieces in collections.pieces -%}
                <div class="col">
                    <div class="card shadow-sm">
                        {# <img src="/assets/images/thumbnails/{{ pieces.data['image-thumb'] }}"> #}
                        <div class="card-body">
                            <p class="card-text">{{ pieces.data.description }}</p>
                            <div class="d-flex justify-content-between align-items-center">
                                <div class="btn-group">
                                    <button type="button" class="btn btn-sm btn-outline-secondary">View</button>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            {%- endfor %}
        </div>
    </div>
</main>